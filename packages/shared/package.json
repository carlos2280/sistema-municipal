{
  "name": "@municipal/shared",
  "version": "1.0.0",
  "description": "Libreria compartida - Schemas DB, tipos, utilidades",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./database": {
      "types": "./dist/database/index.d.ts",
      "import": "./dist/database/index.js"
    },
    "./database/identidad": {
      "types": "./dist/database/identidad/index.d.ts",
      "import": "./dist/database/identidad/index.js"
    },
    "./database/contabilidad": {
      "types": "./dist/database/contabilidad/schemas/index.d.ts",
      "import": "./dist/database/contabilidad/schemas/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/database/index.ts src/database/identidad/index.ts src/database/contabilidad/schemas/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts src/database/index.ts --format esm --dts --watch",
    "seed": "tsx src/seeders/index.ts",
    "migrate": "tsx node_modules/drizzle-kit/bin.cjs migrate",
    "generate": "tsx node_modules/drizzle-kit/bin.cjs generate",
    "db:push": "tsx node_modules/drizzle-kit/bin.cjs push",
    "db:studio": "tsx node_modules/drizzle-kit/bin.cjs studio"
  },
  "keywords": ["municipal", "shared", "drizzle", "schemas"],
  "author": "Carlos Fuentes",
  "license": "ISC",
  "dependencies": {
    "dotenv": "catalog:",
    "drizzle-orm": "catalog:",
    "pg": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "drizzle-kit": "catalog:",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
