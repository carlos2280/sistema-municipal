{
  "name": "mf-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite build --mode development && vite preview --port 5011 --strictPort",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:watch": "vite build --watch --mode development",
    "start": "node server.js",
    "preview": "vite preview --port 5011 --strictPort --mode development",
    "serve-local": "pnpm build && pnpm start",
    "lint": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "check": "biome check --write ./src"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.2",
    "@mui/material": "^7.1.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/node": "^22.18.12",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "globals": "^16.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
