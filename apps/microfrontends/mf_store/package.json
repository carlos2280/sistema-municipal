{
  "name": "mf-store",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite build --mode development && vite preview --port 5010 --strictPort",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:watch": "vite build --watch --mode development",
    "start": "node server.js",
    "preview": "vite preview --port 5010 --strictPort --mode development",
    "serve-local": "pnpm build && pnpm start",
    "lint": "biome lint --write ./src",
    "format": "biome format --write ./src",
    "check": "biome check --write ./src"
  },
  "dependencies": {
    "@reduxjs/toolkit": "catalog:",
    "async-mutex": "^0.5.0",
    "cors": "catalog:",
    "express": "^4.21.2",
    "lucide-react": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-redux": "catalog:",
    "redux-persist": "catalog:",
    "serve": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@module-federation/typescript": "^3.1.3",
    "@originjs/vite-plugin-federation": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "globals": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "^4.5.4"
  }
}
