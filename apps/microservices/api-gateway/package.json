{
  "name": "gateway",
  "version": "1.0.0",
  "description": "API Gateway - Sistema Municipal",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts --no-splitting --clean --minify",
    "start": "node dist/index.js",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check ."
  },
  "author": "Carlos Roberto Fuentes Fuentes",
  "license": "ISC",
  "dependencies": {
    "compression": "^1.8.0",
    "cors": "catalog:",
    "dotenv": "catalog:",
    "express": "catalog:",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "catalog:",
    "pino": "^9.7.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/compression": "^1.8.0",
    "@types/cors": "catalog:",
    "@types/express": "catalog:",
    "@types/express-serve-static-core": "catalog:",
    "@types/node": "catalog:",
    "cross-env": "catalog:",
    "nodemon": "catalog:",
    "pino-pretty": "^13.0.0",
    "tsup": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
